namespace demo{
    policy myPolicy{
        apply firstApplicable
        rule allowGet {
            target clause request.method == "GET" 
            permit
        }
        rule denyAll {
            deny
        }
    }
}
namespace request {
    attribute method {
        id = "request.method"
        type = string
        category = actionCat
    }
    attribute pathname {
        id = "request.pathname"
        type = string
        category = resourceCat
    }
}